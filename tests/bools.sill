not: Bool -> Bool;;
not b =
  case b of
  | False -> True
  | True -> False;;

'c <- 
assert(True && True);
assert(not (False && True));
assert(not (True && False));
assert(not (False && False));

assert(True || True);
assert(False || True);
assert(True || False);
assert(not (False || False));

close 'c;;
